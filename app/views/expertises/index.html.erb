<div ng-app="app">
  <div ng-controller="expertisesCtrl" ng-init="setupExpertiseIndex()">
    <h1>Listing Expertises</h1>

     <p>Basic search by expertise name: <input ng-model='expertiseNameFilter'></p>

    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
          <tr ng-repeat=" expertise in generateList() | filter: { name: expertiseNameFilter }">
            <td>{{ expertise.name }}</td>     
          </tr>
      </tbody>
    </table>
    <br>
    <multiple-autocomplete ng-model="selectedList"
       object-property="name"
       suggestions-arr="expertises">
    </multiple-autocomplete>
  </div>
</div>